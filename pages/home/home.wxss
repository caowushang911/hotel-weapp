/**home.wxss**/
.home--bg {
  background-color: #f3f5f7;
}

.home__head {
  margin-bottom: 12rpx;
}

.home__swiper {
  height: 460rpx;
}

.home--flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  padding: 0 12rpx;
  font-size: 40rpx;
  padding-top: 24rpx;
}

.l-flex {
  display: flex;
  align-items: center;
  color: #606266;
  background-color: #fff;
  padding: 12rpx;
}

.home__icon {
  width: 40rpx;
  height: 40rpx;
  margin-right: 12rpx;
}

.home__btn {
  padding: 8rpx 36rpx;
  border-radius: 5px;
  border: 1rpx solid #68b4ff;
  color: #409eff;
  font-size: 32rpx; 
}

.home__block {
  margin-top: 12rpx;
  margin-bottom: 12rpx;
  padding: 16rpx;
  background-color: #fff;
}

.home-m-l {
  margin-left: 24rpx;
}

.home__title {
  display: block;
  color: #606266;
  padding-left: 8rpx;
  padding-bottom: 12rpx;
  margin-bottom: 24rpx;
  border-bottom: 1rpx solid #EBEEF5;
}

.home__img {
  width: 100%;
  height: 100%;
}

.home__intro {
  line-height: 32rpx;
  font-weight: 530;
  color: #303133;
}

.home__detail {
  display: flex;
  flex-wrap: wrap;
  margin-top: 16rpx;
}

.home__option {
  display: inline-block;
  width: 230rpx;
  color: #303133;
  font-size: 30rpx;
  margin-top: 8rpx;
}

.home__cell {
  display: block;
  font-weight: 550;
  line-height: 64rpx;
}




